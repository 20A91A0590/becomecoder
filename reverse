int print(int n)
{
	if(n==0)
	return n;
	cout<<n<<"/n";
}
int main()
{
	int n,sum=0,r;
	cin>>n;
	while(n>0)
	{
        r=n%10;
        sum=sum+r;
        n=n/10;
	}
	cout<<sum;
}
